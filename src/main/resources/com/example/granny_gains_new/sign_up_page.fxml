<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.granny_gains_new.controller.SignUpController">
    <children>
        <BorderPane prefHeight="1080.0" prefWidth="1920.0" style="-fx-background-color: #FFF;">
            <left>
                <Pane prefHeight="1080.0" prefWidth="689.0" styleClass="linear-grad" stylesheets="@styles.css" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="BackToSignIn" layoutX="37.0" layoutY="890.0" mnemonicParsing="false" onAction="#handleBackToSignIn" prefHeight="71.0" prefWidth="169.0" text="Back">
                     <font>
                        <Font size="1.0" />
                     </font></Button>
               </children></Pane>
            </left>
            <center>
                <Pane prefHeight="669.0" prefWidth="632.0" style="-fx-background-color: #f2f2f2;" BorderPane.alignment="CENTER">
                    <children>
                        <TextField fx:id="tfFirstName" layoutX="106.0" layoutY="339.0" prefHeight="62.0" prefWidth="337.0" promptText="First Name">
                            <font>
                                <Font size="17.0" />
                            </font>
                        </TextField>
                        <TextField fx:id="tfLastName" layoutX="106.0" layoutY="428.0" prefHeight="62.0" prefWidth="337.0" promptText="Last Name">
                            <font>
                                <Font size="17.0" />
                            </font>
                        </TextField>
                        <TextField fx:id="tfEmail" layoutX="106.0" layoutY="519.0" prefHeight="62.0" prefWidth="337.0" promptText="Email">
                            <font>
                                <Font size="17.0" />
                            </font>
                        </TextField>
                        <PasswordField fx:id="tfPassword" layoutX="106.0" layoutY="608.0" prefHeight="62.0" prefWidth="337.0" promptText="Password">
                            <font>
                                <Font size="17.0" />
                            </font>
                        </PasswordField>
                        <Label fx:id="fxlblincorrectdetails" layoutX="106.0" layoutY="520.0" style="-fx-text-fill: red;" text=" ">
                            <font>
                                <Font size="14.0" />
                            </font>
                        </Label>
                        <Button fx:id="Buttonsignup" layoutX="106.0" layoutY="786.0" mnemonicParsing="false" onAction="#handleSignUp" prefHeight="53.0" prefWidth="337.0" style="-fx-background-color: #80dfff; -fx-text-fill: #003366; -fx-background-radius: 5; -fx-border-color: #66ccff;" stylesheets="@styles.css" text="Sign up">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Button>
                        <Label layoutX="106.0" layoutY="262.0" prefHeight="45.0" prefWidth="182.0" text="Sign up">
                            <font>
                                <Font name="System Bold" size="30.0" />
                            </font>
                        </Label>
                  <TextField fx:id="tfPhone" layoutX="106.0" layoutY="697.0" prefHeight="62.0" prefWidth="337.0" promptText="Phone">
                     <font>
                        <Font size="17.0" />
                     </font>
                  </TextField>
                    </children>
                </Pane>
            </center>
        </BorderPane>
    </children>
</AnchorPane>